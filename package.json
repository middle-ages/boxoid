{
  "name": "boxoid",
  "version": "1.0.0",
  "description": "Compose terminal boxes",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "ts": "scripts/run-ts.sh",
    "ts:watch": "scripts/run-ts-watch.sh",
    "clean": "scripts/clean.sh",
    "lint:code": "eslint src README.md doc",
    "lint:fix": "eslint src README.md doc --fix",
    "lint:doc": "markdownlint-cli2 '**/*.md' '#node_modules' '#dist'",
    "lint": "scripts/lint.sh",
    "compile": "tsc --noEmit",
    "repl": "ts-node-esm --interactive",
    "test:unit": "vitest dev",
    "test:unit:verbose": "vitest dev --reporter=verbose",
    "test:unit:run": "vitest run",
    "test:repl": "scripts/test-repl-import.sh",
    "test:should:run": "scripts/run-ts.sh src/devBin/should-run.ts",
    "test:self": "scripts/self-test.sh",
    "test:build:node": "scripts/test-build-node.sh",
    "test:build:node:runtime": "scripts/test-with-runtime.sh",
    "build:node:tsc": "tsc",
    "build:fix:require": "replace-tsconfig-paths",
    "build:node:fix": "scripts/fix-dist-require.sh",
    "build:node:types": "tsc --declaration --emitDeclarationOnly",
    "build:node": "scripts/build-node.sh",
    "build:browser": "vite build",
    "build:preview": "vite preview",
    "build": "scripts/build.sh",
    "js": "scripts/run-dist.sh"
  },
  "author": "Ran Eilam",
  "license": "ISC",
  "publishConfig": {
    "directory": "dist"
  },
  "bin": {
    "demo": "dist/bin/eg.js"
  },
  "dependencies": {
    "@no-day/fp-ts-generators": "^0.1.0-rc.4",
    "@types/ansi-regex": "^5.0.0",
    "@types/color-convert": "^2.0.0",
    "@types/has-ansi": "^5.0.0",
    "@types/slice-ansi": "^5.0.0",
    "@types/wrap-ansi": "^8.0.1",
    "ansi-colors": "^4.1.1",
    "ansi-regex": "^5.0.1",
    "chalk": "4.1.2",
    "color-convert": "^2.0.1",
    "fast-check": "^2.25.0",
    "fast-equals": "^3.0.2",
    "fast-glob": "^3.2.11",
    "fp-ts": "^2.12.1",
    "fp-ts-std": "^0.14.2",
    "has-ansi": "^5.0.1",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "pure-random": "^0.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "slice-ansi": "^5.0.0",
    "string-width": "^4.2.3",
    "wrap-ansi": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.32",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-markdownlint": "^0.3.4",
    "eslint-plugin-md": "^1.0.19",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "markdownlint": "^0.25.1",
    "markdownlint-cli2": "^0.4.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "replace-tsconfig-paths": "^1.2.6",
    "ts-node": "^10.7.0",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.8.0-dev.20220525",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.4"
  }
}
