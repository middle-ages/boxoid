{
  "name": "stacka",
  "version": "1.0.0",
  "description": "Compose terminal boxes",
  "main": "index.js",
  "scripts": {
    "ts": "scripts/run-ts.sh",
    "ts:watch": "scripts/run-ts-watch.sh",
    "clean": "scripts/clean.sh",
    "lint": "eslint src",
    "lint:fix": "eslint src README.md doc --fix",
    "compile": "tsc --noEmit",
    "test:record-examples": "scripts/record-examples.zsh",
    "test": "vitest dev",
    "test:coverage": "vitest --coverage",
    "test:verbose": "vitest dev --reporter=verbose",
    "test:run": "vitest run",
    "test:repl": "scripts/test-repl-import.sh",
    "test:should:run": "scripts/run-ts.sh src/devBin/should-run.ts",
    "build:compile": "tsc",
    "build:fix:require": "replace-tsconfig-paths",
    "build:fix": "scripts/fix-dist-require.sh",
    "build:types": "tsc --declaration --emitDeclarationOnly --project tsconfig.build.json",
    "build": "scripts/build.sh"
  },
  "author": "Ran Eilam",
  "license": "ISC",
  "publishConfig": {
    "directory": "dist"
  },
  "bin": {
    "demo": "dist/src/bin/eg/1-hello-world.js"
  },
  "dependencies": {
    "@types/color-convert": "2.0.0",
    "anser": "^2.1.1",
    "ansis": "^1.5.5",
    "color-blend": "^4.0.0",
    "color-convert": "2.0.1",
    "fast-equals": "^4.0.3",
    "fast-glob": "^3.2.12",
    "fp-ts": "^2.12.3",
    "fp-ts-std": "^0.15.0",
    "io-ts": "^2.2.18",
    "io-ts-types": "^0.5.16",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "string-width": "^4.2.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-check": "^2.6.0",
    "fp-ts-laws": "^0.3.0",
    "prettier": "^2.7.1",
    "replace-tsconfig-paths": "^1.2.6",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.8.3",
    "vite": "^3.1.1",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.23.2"
  }
}
